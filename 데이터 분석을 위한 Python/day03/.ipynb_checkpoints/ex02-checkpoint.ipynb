   "source": [
    "total = 0\n",
    "num = 1\n",
    "\n",
    "while num <= 100:\n",
    "    if num == 50:\n",
    "        break # 반복 멈추기\n",
    "    if num % 2 == 0:\n",
    "        num += 1\n",
    "        continue # 현재 반복을 중단하고 새로 반복 시작(반복 건너뛰기)\n",
    "        \n",
    "    total += num\n",
    "    # num++  파이썬은 증감 연산자 사용 불가\n",
    "    num += 1\n",
    "\n",
    "print(\"합계:\", total)"
   ]
